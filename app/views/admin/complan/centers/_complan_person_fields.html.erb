<div id="<%= dom_id(f.object) %>" class="nested-fields">
    <fieldset style="border: 1px solid grey;margin: 1em 0;padding: 1em;">
        <legend><%=t("admin.complan.center.subtitle_person", i: "")%></legend>
        <%= f.hidden_field :id %>
        <div class="small-12 medium-6  column end"  >
            <%= f.text_field :name %>
        </div>
            <div class="small-12 medium-6 column end"  >
            <%= f.text_field :position %>
        </div>
            <div class="small-12 medium-6  column end"  >
            <%= f.text_field :phone %>
        </div>
            <div class="small-12 medium-6  column end"  >
            <%= f.text_field :email %>
        </div>
        <div class="small-12 medium-12  column end"  >
            <%= f.text_field :address %>
        </div>
        <div class="small-12 medium-4 column action-remove text-right">
            <%=link_to_remove_association  t("admin.complan.center.delete_person") , f, class: "button alert"%>
        </div>
    </fieldset>
</div>