<div id="<%= dom_id(f.object) %>" class="nested-fields">
    <fieldset style="border: 1px solid grey;margin: 1em 0;padding: 1em;">
        <legend><%=t("admin.complan.performance.subtitle_indicator", i: "")%></legend>
        <%= f.hidden_field :id %>
        <div class="small-12 medium-6  column end"  >
            <%= f.text_field :name %>
        </div>
        <div class="small-12 medium-6 column end"  >
            <%= f.text_field :description %>
        </div>
        <div class="small-12 medium-6 column end"  >
            <%= f.text_field :numeric_value %>
        </div>
        <div class="small-12 medium-6 column end"  >
            <%= f.text_field :provided %>
        </div>
        <div class="small-12 medium-6 column end"  >
            <%= f.text_field :done %>
        </div>
           
        <div class="small-12 medium-4 column action-remove text-right">
            <%=link_to_remove_association  t("admin.complan.performance.delete_indicator") , f, class: "button alert"%>
        </div>
    </fieldset>
</div>