<fieldset style="border: 1px solid grey;margin: 1em 0;padding: 1em;">
    <legend><%=t("admin.complan.financing.title_data_file")%></legend>
    <div id="nested_complan_files">
        <%= f.fields_for :complan_files do |ff| %>
            <%= render "complan_file_fields", f: ff %>
        <% end %>

        <div class="js-add-fields-container">
            <div class="small-12 medium-9 column end">
                <%= link_to_add_association t("admin.complan.financing.add_file"),
                                        f, :complan_files, class: "button hollow"  %>
            </div>
        </div>
    </div>       
 </fieldset>