<div id="<%= dom_id(f.object) %>" class="nested-fields">
    <fieldset style="border: 1px solid grey;margin: 1em 0;padding: 1em;">
          <legend><%=t("admin.complan.financing.subtitle_import", i: "")%></legend>
          <%= f.hidden_field :id %>
          <div class="small-12 medium-6  column end"  >
               <%= f.text_field :import %>
          </div>
               <div class="small-12 medium-6 column end"  >
               <%= f.text_field :annuity %>
          </div>
               <div class="small-12 medium-6  column end"  >
               <%= f.text_field :import_start_assing %>
          </div>
               <div class="small-12 medium-6  column end"  >
               <%= f.text_field :import_requested %>
          </div>
          <div class="small-12 medium-12  column end"  >
               <%= f.text_field :import_remaining %>
          </div>

          <%=render "data_credit_modifications", f: f%>

          <div class="small-12 medium-4 column action-remove text-right">
               <%=link_to_remove_association  t("admin.complan.financing.delete_import") , f, class: "button alert"%>
          </div>
    </fieldset>
</div>

