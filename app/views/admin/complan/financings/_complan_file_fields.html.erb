<div id="<%= dom_id(f.object) %>" class="nested-fields">
    <fieldset style="border: 1px solid grey;margin: 1em 0;padding: 1em;">
        <legend><%=t("admin.complan.financing.subtitle_file", i: "")%></legend>
        <%= f.hidden_field :id %>
        <div class="small-12 medium-6  column end"  >
            <%= f.text_field :number_file %>
        </div>
            <div class="small-12 medium-6 column end"  >
            <%= f.text_field :type_file %>
        </div>
            <div class="small-12 medium-6  column end"  >
            <%= f.text_field :proposal_date %>
        </div>
            <div class="small-12 medium-6  column end"  >
            <%= f.text_field :start_date %>
        </div>
        <div class="small-12 medium-12  column end"  >
            <%= f.text_field :approval_date %>
        </div>
        <div class="small-12 medium-12  column end"  >
            <%= f.text_field :accounting_date %>
        </div>
        <div class="small-12 medium-4 column action-remove text-right">
            <%=link_to_remove_association  t("admin.complan.financing.delete_file") , f, class: "button alert"%>
        </div>
    </fieldset>
</div>
